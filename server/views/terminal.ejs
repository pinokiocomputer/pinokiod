<style>
html {
  scroll-behavior: smooth;
  height: 100%;
  overflow: hidden;
}
body {
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
#table > table {
  border-left: 2px solid black;
  padding-left: 5px;
  margin: 10px 0;
}
.highlight {
  background: yellow;
  width: 100px;
}
th {
  background: rgba(0,0,0,0.1);
  padding: 10px;
  font-size: 12px;
}
td {
  font-size: 12px;
  padding: 10px;
}
td.val {
  background: rgba(0,0,0,0.06);
}
td.key {
  background: rgba(0,0,0,0.1);
}
.button {
  cursor: pointer; 
  background: rgba(0,0,0,0.8);
  color: white;
  padding: 5px 10px;
  border-radius: 4px;
}
header {
  position: relative;
  padding: 10px 0;
}
.button:hover {
  
}
.terminal {
  box-sizing: border-box;
}
.xterm .xterm-viewport {
  width: initial !important;
}
.navheader {
  background: var(--dark-bg);
}
.btn {
  background: royalblue;
}
#del-bin {
  color: royalblue;
  cursor: pointer;
  font-weight: bold;
  padding: 0 5px;
  text-decoration: underline;
}
body.frozen {
  overflow: auto !important;
}
