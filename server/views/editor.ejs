<style>
body.frozen {
  display: flex;
  flex-direction: column;
  height: 100%;
}
.terminal-container {
  flex-grow: none !important;
}
.xterm {
  height: 100%;
}
.containers .terminal-container {
  height: auto !important;
}
#table > table {
  border-left: 2px solid black;
  padding-left: 5px;
  margin: 10px 0;
}
.highlight {
  background: yellow;
  width: 100px;
}
th {
  background: rgba(0,0,0,0.1);
  padding: 10px;
  font-size: 12px;
}
td {
  font-size: 12px;
  padding: 10px;
}
td.val {
  background: rgba(0,0,0,0.06);
}
td.key {
  background: rgba(0,0,0,0.1);
